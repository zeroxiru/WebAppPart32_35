@model IEnumerable<Employee>

@{

    ViewBag.Title = "Employees List";

}
<h3>@ViewBag.PageTitle</h3>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Department</th>
            <th>Email</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Department</td>
                <td>@employee.Email</td>
            </tr>
        }
    </tbody>
</table>
@section Scripts
{
    <script src="~/js/custom.js"></script>
}

